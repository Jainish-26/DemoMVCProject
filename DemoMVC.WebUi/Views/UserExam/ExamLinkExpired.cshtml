@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exam Link Expired</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <style>
        body {
            font-family: 'Arial', sans-serif;
            text-align: center;
            margin: 0;
            padding: 0;
            background-color: #f8f9fa;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }

        .container {
            background: #ffffff;
            padding: 30px;
            border-radius: 10px;
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
            max-width: 500px;
            width: 100%;
        }

        .icon {
            font-size: 60px;
            color: red;
            margin-bottom: 20px;
        }

        .expired {
            color: #dc3545;
            font-size: 24px;
            font-weight: bold;
        }

        .info {
            font-size: 16px;
            color: #6c757d;
            margin-top: 10px;
        }
    </style>
</head>
<body>

    <div class="container">
        <i class="fas fa-exclamation-triangle icon"></i>
        <h2 class="expired">Exam Link Expired</h2>
        <p class="info">Sorry, this exam link is no longer available. If you need assistance, please contact support.</p>
    </div>

</body>
</html>
